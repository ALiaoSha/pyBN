network unknown {
}
variable draw {
	type discrete [ 2 ] { red, blue };
}
variable lie {
	type discrete [ 2 ] { yes, no };
}
variable personal_action {
	type discrete [ 2 ] { red, blue };
}
probability ( draw ) {
	table 0.67, 0.33;
}
probability ( lie ) {
	table 0.001, 0.999;
}
probability ( personal_action | draw, lie ) {
	(red, yes) 1.0, 0.0;
	(blue, yes) 0.0, 1.0;
	(red, no) 1.0, 0.0;
	(blue, no) 0.0, 1.0;
}